#include<stdio.h>
int main()
{
	int x,y,z;
	printf("Enter first number : ");
	scanf("%d",&x);
	printf("Enter second number : ");
	scanf("%d",&y);
	printf("Enter third number : ");
	scanf("%d",&z);
	
	
	if (z<x && z<y)
	{
		printf("%d,",x);
		printf("%d",y);
	}
	else if (y<x && y<z)
	{
		printf("%d,",x);
		printf("%d",z);
	}
	else if (x<z && x<y)
	{
		printf("%d,",y);
		printf("%d",z);
	}
}
